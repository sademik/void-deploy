if [ -d /etc/X11/xinit/xinitrc.d ] ; then
for f in /etc/X11/xinit/xinitrc.d/?*.sh ; do
[ -x "$f" ] && . "$f"
done
unset f
fi
setxkbmap us &
exec dwm & wmpid=
exec slstatus &
exec picom --no-vsync &
sleep 5
xset r rate 175
~/void-deploy/scripts/.set_monitor.sh
feh --bg-fill ~/void-deploy/wallpapers/spiritedaway.png
~/void-deploy/scripts/.set_time.sh
wait 
